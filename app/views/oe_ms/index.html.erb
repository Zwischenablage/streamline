<p id="notice"><%= notice %></p>

<h1>Oems</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oems.each do |oem| %>
      <tr>
        <td><%= oem.name %></td>
        <td><%= link_to 'Show', oem %></td>
        <td><%= link_to 'Edit', edit_oem_path(oem) %></td>
        <td><%= link_to 'Destroy', oem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oem', new_oem_path %>
